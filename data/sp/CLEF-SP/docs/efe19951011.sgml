    Cantabria        0,1             3,6               4,2
    C.-La Mancha     0,4             3,9               4,7
    C. y León        0,3             3,6               4,3
    Cataluña         0,4             3,4               4,3
    C. Valenciana    0,4             3,5               4,2
    Extremadura      0,3             3,3               4,3
    Galicia          0,3             3,6               4,5
    Madrid           0,6             3,1               3,9
    Murcia           0,4             3,8               4,7
    Navarra          0,4             4,4               5,3
    País Vasco       0,3             3,7               4,6
    La Rioja         0,4             3,7               4,9
    ----------------------------------------------------------
    Ceuta            1,1             3,1               3,9
    Melilla          0,0             3,7               3,9
    ==========================================================
    INDICE GENERAL   0,4             3,5               4,4
